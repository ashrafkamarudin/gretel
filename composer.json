{
  "name": "glhd/gretel",
  "description": "",
  "keywords": [
    "laravel",
    "breadcrumbs"
  ],
  "authors": [
    {
      "name": "Chris Morrell",
      "homepage": "http://www.cmorrell.com"
    }
  ],
  "license": "MIT",
  "require": {
    "illuminate/support": "^8.0|^9.0|^10.0|dev-master",
    "illuminate/routing": "^8.0|^9.0|^10.0|dev-master",
    "illuminate/view": "^8.0|^9.0|^10.0|dev-master",
    "laravel/serializable-closure": "^1.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.21|^7.0|dev-master",
    "friendsofphp/php-cs-fixer": "^3.0",
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^9.5",
    "glhd/laravel-dumper": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Glhd\\Gretel\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Glhd\\Gretel\\Tests\\": "tests/",
      "Glhd\\Gretel\\Tests\\Database\\Factories\\": "tests/database/factories/"
    }
  },
  "scripts": {
    "fix-style": "vendor/bin/php-cs-fixer fix",
    "check-style": "vendor/bin/php-cs-fixer fix --diff --dry-run"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Glhd\\Gretel\\Support\\GretelServiceProvider"
      ],
      "aliases": {
        "Gretel": "Glhd\\Gretel\\Support\\Facades\\Gretel"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
